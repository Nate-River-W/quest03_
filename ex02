#include <stdio.h>
char* my_strcpy(char* param_1, char* param_2)
{	int i,t;
	for(i=0; param_2[i] != '\0'; i++)
	{
		t = param_2[i];
		param_1[i] = param_2[i];
		param_2[i] = t;
	
	}
	
	
	
	
	
	
	return param_2, param_1;
}






int main() {
  char dst[100] = {};
  char *src     = "RaInB0w d4Sh! ";
  
  printf("my_strcpy -> %s\n", my_strcpy(dst, src));
  return 0;
}
