#include <stdio.h>


char* my_strchr(char* param_1, char param_2)
{	char* list = "";
	int i;
	for(i = 0; param_1[i] != '\0'; i++)
	{	
		if(param_2 == param_1[i] )
		{
			list = &param_1[i];
			return list;
		}	
		
	}
	
	
	
}
int main() 
{
	char* p1 = "abc";
	char p2 = 'd';
	printf("%s", my_strchr(p1,p2));
	return 0;
}
